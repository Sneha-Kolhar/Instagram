
<html>
<head>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery-validation/dist/additional-methods.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/js/site.js"></script>
    <script>

        function search() {
            var key = document.getElementById("searchKey");
            var link = "/InstagramAccount/ListUsers/?nsearch=" + key.value.replaceAll(" ", "");
            console.log(link);
            console.log(key);
            $("#Result").load(link);
        }

     
    </script>
</head>
<body>


    <div>
        <input id="searchKey" value="" placeholder="Find Someone..." class="col-3" name="nsearch" />
        <button onclick="search()" class="col-1" id="btnsearch"> Search </button>
    </div>
    <div>
        @Html.ActionLink("View Friends", "viewFriendRequest", "Friends")
    </div>
    <div id="Result"> </div>
</body>
</html>





